	"tokenColors": [
        {
            "settings": {
                "foreground": "#D5CED9",
                "background": "#23262E"
            }
        },
        {
            "name": "Comment color",
            "scope": [
                "comment",
                "markup.quote.markdown",
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#5824247e"//#5824247e--red// #a7a0a0cc--gray(default)
            }
        },
        {
            "name": "Text Color",
            "scope": [
                "meta.template.expression.js",
                "constant.name.attribute.tag.jade",
                "punctuation.definition.metadata.markdown",
                "punctuation.definition.string.end.markdown",
                "punctuation.definition.string.begin.markdown"
            ],
            "settings": {
                "foreground": "#61f7ac"
            }
        },
        {
            "name": "Cyan",
            "scope": [
                "variable",
                "support.variable",
                "entity.name.tag.yaml",
